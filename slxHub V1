    lineframe3.AnchorPoint = Vector2.new(0.5, 0.5)
    lineframe3.Position = UDim2.new(0.5, 0, 0.5, 0)
    lineframe3.BorderSizePixel = 0
    lineframe3.Size = UDim2.new(0, 155, 0, 13)
    lineframe3.Font = Enum.Font.GothamSemibold
    lineframe3.Text = tostring(text)
    lineframe3.TextColor3 = AZUL_OSCURO
    lineframe3.TextSize = 12.000
    
    
    local lineframe4 = Instance.new("TextLabel")

    lineframe4.Parent = lineframe
    lineframe4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    lineframe4.BackgroundTransparency = 1
    lineframe4.AnchorPoint = Vector2.new(0.5, 0.5)
    lineframe4.Position = UDim2.new(0.5, 0, 0.5, 0)
    lineframe4.BorderSizePixel = 0
    lineframe4.Size = UDim2.new(0, 155, 0, 13)
    lineframe4.Font = Enum.Font.GothamSemibold
    lineframe4.Text = tostring(text)
    lineframe4.TextColor3 = Color3.fromRGB(255, 255, 255)
    lineframe4.TextSize = 12.000
    lineframe4.BackgroundTransparency = 1
end

function ems : Label(text)

    local labelfuc = {}
    local  Labelxd = Instance.new("TextLabel")

    Labelxd.Parent = ScolPage2
    Labelxd.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Labelxd.BackgroundTransparency = 1
    Labelxd.AnchorPoint = Vector2.new(0.5, 0.5)
    Labelxd.Position = UDim2.new(0.5, 0, 0.8, 0)
    Labelxd.BorderSizePixel = 0
    Labelxd.Size = UDim2.new(0, 155, 0, 13)
    Labelxd.Font = Enum.Font.GothamSemibold
    Labelxd.Text = tostring(text)
    Labelxd.TextColor3 = Color3.fromRGB(255, 255, 255)
    Labelxd.TextSize = 12.000

    function  labelfuc : Change(text2)
        Labelxd.Text = tostring(text2)
    end
    return  labelfuc
end


return ems
end
return page
end
return tap
end

local doc = create:Win()

local Home = doc:addtap("Home")
local Page1 = Home:addpage()
local ems1 = Page1:addems()

ems1:Line("Movement")

ems1:Toggle("Infinity Jump", false, function(value)
	if value then
		LocalPlayer.Character.Humanoid.JumpPower = 0
		LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
			if LocalPlayer.Character.Humanoid.JumpPower ~= 0 then
				LocalPlayer.Character.Humanoid.JumpPower = 0
			end
		end)
	else
		LocalPlayer.Character.Humanoid.JumpPower = 50
	end
end)

local flytoggle = false
local FlyBindable = Instance.new("BindableFunction")

ems1:Toggle("Fly", false, function(value)
    flytoggle = value
    FlyBindable:Invoke(value)
end)

FlyBindable.OnInvoke = function(value)
    if value then
        LocalPlayer.Character.Humanoid.PlatformStand = true
        LocalPlayer.Character.Humanoid.WalkSpeed = 0
        LocalPlayer.Character.Humanoid.JumpPower = 0
        LocalPlayer.Character.HumanoidRootPart.Anchored = true
    else
        LocalPlayer.Character.Humanoid.PlatformStand = false
        LocalPlayer.Character.Humanoid.WalkSpeed = 16
        LocalPlayer.Character.Humanoid.JumpPower = 50
        LocalPlayer.Character.HumanoidRootPart.Anchored = false
    end
end)

RunService.RenderStepped:Connect(function()
    if flytoggle and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local HRP = LocalPlayer.Character.HumanoidRootPart
        local camCF = game.Workspace.CurrentCamera.CFrame
        local moveVector = Vector3.new(0, 0, 0)

        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveVector = moveVector + camCF.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveVector = moveVector - camCF.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveVector = moveVector + camCF.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveVector = moveVector - camCF.RightVector
        end

        HRP.CFrame = HRP.CFrame + Vector3.new(moveVector.X, 0, moveVector.Z) * 0.5
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            HRP.CFrame = HRP.CFrame + Vector3.new(0, 0.5, 0)
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
            HRP.CFrame = HRP.CFrame - Vector3.new(0, 0.5, 0)
        end
    end
end)


ems1:Line("Local Player")

-- MODIFICADO: El slider ahora va hasta 500 para el speed hack
ems1:Slider("WalkSpeed", 16, 500, 16, function(value)
	LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

ems1:Slider("JumpPower", 50, 100, 50, function(value)
	LocalPlayer.Character.Humanoid.JumpPower = value
end)

local TeleportTab = doc:addtap("Teleport")
local Page2 = TeleportTab:addpage()
local ems2 = Page2:addems()

ems2:Line("Aimbot")

local aimbotEnabled = false

ems2:Toggle("Aimbot", false, function(value)
    aimbotEnabled = value
end)

ems2:Button("Kill All", function()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local HRP = player.Character.HumanoidRootPart
            if HRP then
                LocalPlayer.Character.HumanoidRootPart.CFrame = HRP.CFrame * CFrame.new(0, 0, 10)
                wait(0.1)
                LocalPlayer.Character.HumanoidRootPart.CFrame = HRP.CFrame * CFrame.new(0, 0, 10)
            end
        end
    end
end)

-- Aimbot Logic (simplistic example)
RunService.Stepped:Connect(function()
    if aimbotEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local closestTarget = nil
        local minDistance = math.huge

        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local targetHRP = player.Character.HumanoidRootPart
                local distance = (LocalPlayer.Character.HumanoidRootPart.Position - targetHRP.Position).Magnitude

                if distance < minDistance and distance < 50 then -- Rango de aimbot (ej. 50 studs)
                    minDistance = distance
                    closestTarget = targetHRP
                end
            end
        end

        if closestTarget then
            local targetPosition = closestTarget.Position
            local head = closestTarget.Parent:FindFirstChild("Head")
            if head then
                targetPosition = head.Position
            end

            -- Calcular la direcci√≥n hacia el objetivo
            local direction = (targetPosition - LocalPlayer.Character.HumanoidRootPart.Position).Unit
            
            -- Calcular la CFrame para apuntar
            local lookAt = CFrame.new(LocalPlayer.Character.HumanoidRootPart.Position, targetPosition)
            
            -- Aplicar la rotaci√≥n (s√≥lo en Y) a la c√°mara y al personaje si es necesario
            game.Workspace.CurrentCamera.CFrame = lookAt
            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(LocalPlayer.Character.HumanoidRootPart.Position) * CFrame.Angles(0, lookAt:ToOrientation(), 0)
        end
    end
end)

ems2:Line("External Hub") -- NUEVA FUNCI√ìN A√ëADIDA

-- Bot√≥n para cargar el hub con el loadstring proporcionado
ems2:Button("Load SLX Hub", function()
    -- Ejecuta el loadstring del hub
    pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ZLXvip/slxHub/refs/heads/main/slxHub V1"))()
    end)
end)
