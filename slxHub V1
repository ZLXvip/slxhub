    local TextButton_3_Toggle = Instance.new("TextButton")


    TextButton_3_Toggle.Parent = TextButton_2_Toggle
    TextButton_3_Toggle.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    TextButton_3_Toggle.BorderColor3 = AZUL_OSCURO 
    TextButton_3_Toggle.BorderSizePixel = 0
    TextButton_3_Toggle.AnchorPoint = Vector2.new(0.5, 0.5)
    TextButton_3_Toggle.Position = UDim2.new(0.5, 0, 0.5, 0)
    TextButton_3_Toggle.Size = UDim2.new(0, 17, 0, 17)
    TextButton_3_Toggle.Font = Enum.Font.SourceSans
    TextButton_3_Toggle.Text = " "
    TextButton_3_Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton_3_Toggle.TextSize = 12.000
    TextButton_3_Toggle.AutoButtonColor = false 

    local ImageLabel_Toggle = Instance.new("ImageButton")


    ImageLabel_Toggle.Parent = TextButton_2_Toggle
    ImageLabel_Toggle.BackgroundTransparency = 1
    ImageLabel_Toggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ImageLabel_Toggle.Size = UDim2.new(0, 0, 0, 0)
    ImageLabel_Toggle.Image = "rbxassetid://5880482965" 
    ImageLabel_Toggle.AnchorPoint = Vector2.new(0.5, 0.5)
    ImageLabel_Toggle.Position = UDim2.new(0.47, 0, 0.5, 0)


    local FrameToggle = Instance.new("UICorner")
            
    FrameToggle.CornerRadius = UDim.new(0, 4)
    FrameToggle.Name = ""
    FrameToggle.Parent = ToggleButton

    local FrameToggle1 = Instance.new("UICorner")
            
    FrameToggle1.CornerRadius = UDim.new(0, 4)
    FrameToggle1.Name = ""
    FrameToggle1.Parent = TextButton_2_Toggle

    local FrameToggle2 = Instance.new("UICorner")
            
    FrameToggle2.CornerRadius = UDim.new(0, 4)
    FrameToggle2.Name = ""
    FrameToggle2.Parent = ImageLabel_Toggle

    local FrameToggle3 = Instance.new("UICorner")
            
    FrameToggle3.CornerRadius = UDim.new(0, 4)
    FrameToggle3.Name = ""
    FrameToggle3.Parent = TextButton_3_Toggle

    TextButton_3_Toggle.MouseButton1Click:Connect(function (  )
        if togdoc.boolen ==false then
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 23, 0, 23), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
        else
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
     --       ImageLabel_Toggle.Visible = false
        end
        togdoc.boolen = not togdoc.boolen
        pcall(callback,togdoc.boolen)
    end)

    ImageLabel_Toggle.MouseButton1Click:Connect(function (  )
        if togdoc.boolen ==false then
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 23, 0, 23), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
        else
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
     --       ImageLabel_Toggle.Visible = false
        end
        togdoc.boolen = not togdoc.boolen
        pcall(callback,togdoc.boolen)
    end)

    TextButton_2_Toggle.MouseButton1Click:Connect(function (  )
        if togdoc.boolen ==false then
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 19, 0, 19), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
        else
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
     --       ImageLabel_Toggle.Visible = false
        end
        togdoc.boolen = not togdoc.boolen
        pcall(callback,togdoc.boolen)
    end)


    ToggleButton.MouseButton1Click:Connect(function (  )
        if togdoc.boolen ==false then
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 19, 0, 19), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
        else
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
     --       ImageLabel_Toggle.Visible = false
        end
        togdoc.boolen = not togdoc.boolen
        pcall(callback,togdoc.boolen)
    end)

        if de == true then
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 27, 0, 27), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            wait(0.1)
            ImageLabel_Toggle:TweenSizeAndPosition(UDim2.new(0, 19, 0, 19), UDim2.new(0.5, 0, 0.5, 0), "In", "Bounce", 0.1, true)
            togdoc.boolen = not togdoc.boolen
            pcall(callback,togdoc.boolen)
        end
    end

function ems:DropDown(text,text2,list,callback)


    local checkdrop = {checkscol = false ; }
    local DropFrame = Instance.new("Frame")
    local dropfuc = {}

    DropFrame.Name = "DropFrame"
    DropFrame.Parent = ScolPage2
    DropFrame.BackgroundColor3 = AZUL_OSCURO 
    DropFrame.BorderSizePixel = 0
    DropFrame.BackgroundTransparency = 1
    DropFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropFrame.Size = UDim2.new(0, 155, 0, 45)
    DropFrame.BackgroundTransparency = 1
    DropFrame.ClipsDescendants = true 

    local TextLabelDrop = Instance.new("TextLabel") 

    TextLabelDrop.Parent = DropFrame
    TextLabelDrop.Name = "TextLabelDrop"
    TextLabelDrop.BackgroundColor3 = AZUL_OSCURO 
    TextLabelDrop.BackgroundTransparency = 1
    TextLabelDrop.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabelDrop.Position = UDim2.new(0.5, 0, 0.2, 0)
    TextLabelDrop.Size = UDim2.new(0, 155, 0, 15)
    TextLabelDrop.Font = Enum.Font.GothamSemibold
    TextLabelDrop.Text = text
    TextLabelDrop.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabelDrop.TextSize = 10.000
    TextLabelDrop.BorderSizePixel = 0
    TextLabelDrop.ClipsDescendants = true 

    local FrameDrop = Instance.new("Frame")
    
    FrameDrop.Name = "FrameDrop"
    FrameDrop.Parent = DropFrame
    FrameDrop.BackgroundColor3 = AZUL_OSCURO 
    FrameDrop.BorderSizePixel = 0
    FrameDrop.BackgroundTransparency = 1
    FrameDrop.AnchorPoint = Vector2.new(0.5, 0.5)
    FrameDrop.Position = UDim2.new(0.5, 0, 0.75, 0)
    FrameDrop.Size = UDim2.new(0, 155, 0, 27)
    FrameDrop.BackgroundTransparency = 0
    FrameDrop.ClipsDescendants = true 

    local DropButton = Instance.new("TextButton")

    DropButton.Parent = FrameDrop
    DropButton.BackgroundColor3 = AZUL_OSCURO 
    DropButton.BackgroundTransparency = 1
    DropButton.AnchorPoint = Vector2.new(0.5, 0.5)
    DropButton.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropButton.Size = UDim2.new(0, 155, 0, 24)
    DropButton.Font = Enum.Font.GothamSemibold
    DropButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    DropButton.TextSize = 11.000
    DropButton.TextWrapped = true
    DropButton.Text = ""
    DropButton.BorderSizePixel = 0 
    DropButton.AutoButtonColor = false 
    DropButton.ClipsDescendants = true 

    local TextLabelDrop2 = Instance.new("TextLabel") 

    TextLabelDrop2.Parent = DropButton
    TextLabelDrop2.Name = "TextLabelDrop"
    TextLabelDrop2.BackgroundColor3 = AZUL_OSCURO 
    TextLabelDrop2.BackgroundTransparency = 1
    TextLabelDrop2.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabelDrop2.Position = UDim2.new(0.5, 0, 0.5, 0)
    TextLabelDrop2.Size = UDim2.new(0, 155, 0, 22)
    TextLabelDrop2.Font = Enum.Font.GothamSemibold
    TextLabelDrop2.Text = text2.." : "
    TextLabelDrop2.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabelDrop2.TextSize = 10.000
    TextLabelDrop2.BorderSizePixel = 0

    local FrameDrop2 = Instance.new("UICorner")
            
    FrameDrop2.CornerRadius = UDim.new(0, 4)
    FrameDrop2.Name = ""
    FrameDrop2.Parent = FrameDrop


    local TextLabelDrop4 = Instance.new("TextLabel") 

    TextLabelDrop4.Parent = DropButton
    TextLabelDrop4.Name = "TextLabelDrop"
    TextLabelDrop4.BackgroundColor3 = AZUL_OSCURO 
    TextLabelDrop4.BackgroundTransparency = 1
    TextLabelDrop4.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabelDrop4.Position = UDim2.new(0.5, 0, 0.5, 0)
    TextLabelDrop4.Size = UDim2.new(0, 155, 0, 22)
    TextLabelDrop4.Font = Enum.Font.GothamSemibold
    TextLabelDrop4.Text = "..."
    TextLabelDrop4.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabelDrop4.TextSize = 10.000
    TextLabelDrop4.BorderSizePixel = 0
    TextLabelDrop4.Visible = false 


    local DropScol = Instance.new("ScrollingFrame")
    local UIListLayout_DropScol = Instance.new("UIListLayout")

    DropScol.Name = "DropScol"
    DropScol.Parent = DropFrame
    DropScol.Active = true
    DropScol.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    DropScol.BorderSizePixel = 0
    DropScol.Size = UDim2.new(0, 155, 0, 0)
    DropScol.Position = UDim2.new(0.5, 0, 0.5, 25)
    DropScol.ScrollBarThickness = 3
    DropScol.ScrollBarImageColor3 =  Color3.fromRGB(235, 235, 235)
    DropScol.BackgroundTransparency = 1
    
    UIListLayout_DropScol.Parent = DropScol
    UIListLayout_DropScol.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout_DropScol.Padding = UDim.new(0, 5)

    
    local UIPadding_DropScol = Instance.new("UIPadding")

    UIPadding_DropScol.Parent = DropScol
    UIPadding_DropScol.PaddingTop = UDim.new(0, 5)


    for i,v in next , list do 
        
        local ListButton = Instance.new("TextButton")

        ListButton.Parent = DropScol
        ListButton.BackgroundColor3 = AZUL_OSCURO 
        ListButton.BackgroundTransparency = 0
        ListButton.AnchorPoint = Vector2.new(0.5, 0.5)
        ListButton.Position = UDim2.new(0.5, 0, 0.5, 0)
        ListButton.Size = UDim2.new(1, -10, 0, 20)
        ListButton.Font = Enum.Font.GothamSemibold
        ListButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        ListButton.TextSize = 11.000
        ListButton.TextWrapped = true
        ListButton.Text = v
        ListButton.BorderSizePixel = 0 
        ListButton.AutoButtonColor = false 

        local ListFrame = Instance.new("UICorner")
            
        ListFrame.CornerRadius = UDim.new(0, 4)
        ListFrame.Name = ""
        ListFrame.Parent = ListButton

        ListButton.MouseButton1Click:Connect(function (  )
            TextLabelDrop2.Text = v.. " : "
            TextLabelDrop4.Text = v
            checkdrop.checkscol = not checkdrop.checkscol
            FrameDrop:TweenSize(UDim2.new(0, 155, 0, 27), "Out", "Quad", 0.2, true)
            DropScol:TweenSize(UDim2.new(0, 155, 0, 0), "Out", "Quad", 0.2, true)
            wait(0.2)
            pcall(callback,v)
        end)
    end
    

    DropButton.MouseButton1Click:Connect(function (  )
        if checkdrop.checkscol == false then
            FrameDrop:TweenSize(UDim2.new(0, 155, 0, 150), "Out", "Quad", 0.2, true)
            DropScol:TweenSize(UDim2.new(0, 155, 0, 120), "Out", "Quad", 0.2, true)
        else
            FrameDrop:TweenSize(UDim2.new(0, 155, 0, 27), "Out", "Quad", 0.2, true)
            DropScol:TweenSize(UDim2.new(0, 155, 0, 0), "Out", "Quad", 0.2, true)
        end
        checkdrop.checkscol = not checkdrop.checkscol
    end)
    
    function dropfuc : Change(new)
        TextLabelDrop2.Text = tostring(new)
    end
    return dropfuc
end


function ems:Slider(text,min,max,default,callback)

    local DropFrame = Instance.new("Frame")
    local sliderfuc = {}

    DropFrame.Name = "DropFrame"
    DropFrame.Parent = ScolPage2
    DropFrame.BackgroundColor3 = AZUL_OSCURO 
    DropFrame.BorderSizePixel = 0
    DropFrame.BackgroundTransparency = 1
    DropFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropFrame.Size = UDim2.new(0, 155, 0, 45)
    DropFrame.BackgroundTransparency = 1

    local TextLabelDrop = Instance.new("TextLabel") 

    TextLabelDrop.Parent = DropFrame
    TextLabelDrop.Name = "TextLabelDrop"
    TextLabelDrop.BackgroundColor3 = AZUL_OSCURO 
    TextLabelDrop.BackgroundTransparency = 1
    TextLabelDrop.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabelDrop.Position = UDim2.new(0.5, 0, 0.2, 0)
    TextLabelDrop.Size = UDim2.new(0, 155, 0, 15)
    TextLabelDrop.Font = Enum.Font.GothamSemibold
    TextLabelDrop.Text = text
    TextLabelDrop.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabelDrop.TextSize = 10.000
    TextLabelDrop.BorderSizePixel = 0
    TextLabelDrop.ClipsDescendants = true 

    local FrameDrop = Instance.new("Frame")
    
    FrameDrop.Name = "FrameDrop"
    FrameDrop.Parent = DropFrame
    FrameDrop.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    FrameDrop.BorderSizePixel = 0
    FrameDrop.BackgroundTransparency = 0
    FrameDrop.AnchorPoint = Vector2.new(0.5, 0.5)
    FrameDrop.Position = UDim2.new(0.5, 0, 0.75, 0)
    FrameDrop.Size = UDim2.new(0, 155, 0, 20)
    FrameDrop.BackgroundTransparency = 0
    FrameDrop.ClipsDescendants = true 


    local Sliderframe = Instance.new("Frame")
    
    Sliderframe.Name = "Sliderframe"
    Sliderframe.Parent = FrameDrop
    Sliderframe.BackgroundColor3 = AZUL_OSCURO 
    Sliderframe.BorderSizePixel = 0
    Sliderframe.BackgroundTransparency = 0
    Sliderframe.AnchorPoint = Vector2.new(0, 0.5)
    Sliderframe.Position = UDim2.new(0, 0, 0.5, 0)
    Sliderframe.Size = UDim2.new(0, 0, 0, 20)
    Sliderframe.BackgroundTransparency = 0
    
    local TextLabelSlider = Instance.new("TextLabel") 

    TextLabelSlider.Parent = DropFrame
    TextLabelSlider.Name = "TextLabelDrop"
    TextLabelSlider.BackgroundColor3 = AZUL_OSCURO 
    TextLabelSlider.BackgroundTransparency = 1
    TextLabelSlider.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabelSlider.Position = UDim2.new(0.5, 0, 0.75, 0)
    TextLabelSlider.Size = UDim2.new(0, 155, 0, 20)
    TextLabelSlider.Font = Enum.Font.GothamSemibold
    TextLabelSlider.Text = default
    TextLabelSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabelSlider.TextSize = 10.000
    TextLabelSlider.BorderSizePixel = 0
    TextLabelSlider.BackgroundTransparency = 1


    local FrameDrop2 = Instance.new("UICorner")
            
    FrameDrop2.CornerRadius = UDim.new(0, 4)
    FrameDrop2.Name = ""
    FrameDrop2.Parent = FrameDrop
    
    local FrameDrop3 = Instance.new("UICorner")
            
    FrameDrop3.CornerRadius = UDim.new(0, 4)
    FrameDrop3.Name = ""
    FrameDrop3.Parent = Sliderframe


    local drag = false 
    local drag2 = false
    local newvalue = default 

    Sliderframe.Size = UDim2.new(0, default, 0, 20)
    
    local function onDrag(input)
        local mousePos = input.Position
        local pos = math.clamp(mousePos.X - FrameDrop.AbsolutePosition.X, 0, FrameDrop.AbsoluteSize.X)
        local value = math.round((pos / FrameDrop.AbsoluteSize.X) * (max - min) + min)

        Sliderframe.Size = UDim2.new(0, pos, 0, 20)
        TextLabelSlider.Text = value
        newvalue = value

        pcall(callback, value)
    end

    FrameDrop.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            drag = true
            onDrag(input)
        end
    end)
    
    FrameDrop.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement and drag then
            onDrag(input)
        end
    end)

    FrameDrop.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            drag = false
        end
    end)

    function sliderfuc : Change(new)
        TextLabelSlider.Text = tostring(new)
        newvalue = new
    end
    return sliderfuc
end


function ems:TextBox(text,text2,callback)
    
    local textboxdoc = {}
    local DropFrame = Instance.new("Frame")

    DropFrame.Name = "DropFrame"
    DropFrame.Parent = ScolPage2
    DropFrame.BackgroundColor3 = AZUL_OSCURO 
    DropFrame.BorderSizePixel = 0
    DropFrame.BackgroundTransparency = 1
    DropFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    DropFrame.Size = UDim2.new(0, 155, 0, 45)
    DropFrame.BackgroundTransparency = 1

    local TextLabelDrop = Instance.new("TextLabel") 

    TextLabelDrop.Parent = DropFrame
    TextLabelDrop.Name = "TextLabelDrop"
    TextLabelDrop.BackgroundColor3 = AZUL_OSCURO 
    TextLabelDrop.BackgroundTransparency = 1
    TextLabelDrop.AnchorPoint = Vector2.new(0.5, 0.5)
    TextLabelDrop.Position = UDim2.new(0.5, 0, 0.2, 0)
    TextLabelDrop.Size = UDim2.new(0, 155, 0, 15)
    TextLabelDrop.Font = Enum.Font.GothamSemibold
    TextLabelDrop.Text = text
    TextLabelDrop.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabelDrop.TextSize = 10.000
    TextLabelDrop.BorderSizePixel = 0
    TextLabelDrop.ClipsDescendants = true 

    local FrameDrop = Instance.new("Frame")
    
    FrameDrop.Name = "FrameDrop"
    FrameDrop.Parent = DropFrame
    FrameDrop.BackgroundColor3 = AZUL_OSCURO 
    FrameDrop.BorderSizePixel = 0
    FrameDrop.BackgroundTransparency = 1
    FrameDrop.AnchorPoint = Vector2.new(0.5, 0.5)
    FrameDrop.Position = UDim2.new(0.5, 0, 0.75, 0)
    FrameDrop.Size = UDim2.new(0, 155, 0, 20)
    FrameDrop.BackgroundTransparency = 0
    FrameDrop.ClipsDescendants = true 

    local TextBox = Instance.new("TextBox")

    TextBox.Parent = FrameDrop
    TextBox.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    TextBox.BackgroundTransparency = 0
    TextBox.AnchorPoint = Vector2.new(0.5, 0.5)
    TextBox.Position = UDim2.new(0.5, 0, 0.5, 0)
    TextBox.Size = UDim2.new(0, 155, 0, 20)
    TextBox.Font = Enum.Font.GothamSemibold
    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextBox.TextSize = 10.000
    TextBox.TextWrapped = true
    TextBox.Text = text2
    TextBox.BorderSizePixel = 0 
    TextBox.ClearTextOnFocus = false 

    local FrameDrop2 = Instance.new("UICorner")
            
    FrameDrop2.CornerRadius = UDim.new(0, 4)
    FrameDrop2.Name = ""
    FrameDrop2.Parent = FrameDrop

    TextBox.FocusLost:Connect(function (enter)
        if enter then
            pcall(callback,TextBox.Text)
        end
    end)

    function textboxdoc : Change(text)
        TextBox.Text = tostring(text)
    end
    return textboxdoc
end


